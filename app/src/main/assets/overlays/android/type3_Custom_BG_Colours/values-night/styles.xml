        <item name="android:panelColorBackground">@*android:color/primary_material_dark</item>
        <item name="android:alertDialogTheme">@*android:style/Theme.DeviceDefault.Dialog.Alert</item>
        <item name="android:colorControlNormal">?*android:textColorPrimary</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_device_default_settings</item>
        <item name="android:colorAccent">@*android:color/accent_device_default_dark</item>
        <item name="android:colorBackgroundFloating">@*android:color/primary_material_dark</item>
        <item name="android:colorSecondary">@*android:color/secondary_device_default_settings</item>
        <item name="android:colorError">@*android:color/error_color_device_default_dark</item>
        <item name="android:forceDarkAllowed">false</item>
    </style>
    <style name="Theme.DeviceDefault.Resolver" parent="@*android:style/Theme.DeviceDefault.ResolverCommon">
        <item name="android:textColorSecondary">@*android:color/resolver_text_color_secondary_dark</item>
